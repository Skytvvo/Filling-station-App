<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:FogOilAssistant.Components.Dictionary.Pages"
                    xmlns:svgc="http://sharpvectors.codeplex.com/svgc/">
                    
    <Style x:Key="support_logo" TargetType="svgc:SvgViewbox">
        <Setter Property="Margin" Value="20"/>
    </Style>
    <Style x:Key="support_description" TargetType="Grid">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

    <Style x:Key="support_panel" TargetType="WrapPanel">
        <Setter Property="Background" Value="#FF222121"/>
        
    </Style>
    
</ResourceDictionary>